<%= link_to venue_path(venue), class: "text-decoration-none" do %>
  <div class="card-trip">
    <img src="<%= cl_image_path venue.photo.key, height: 300, crop: :fit %>" />
    <div class="card-trip-infos">
      <div class= "content text-decoration-none">
        <h2 class= "title"><%= venue.name %></h2>
        <p class= "description"><%= venue.overview %></p>
      </div>
      <h2 class="card-trip-pricing"></h2>
    </div>
  </div>
<% end %>
